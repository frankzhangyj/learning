<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.microsoft.boot3.mapper.EmpMapper">

    <select id="queryAll" resultType="emp">
        select * from t_emp_new
    </select>

    <delete id="delete">
        delete from t_emp_new where emp_id = #{id}
    </delete>

</mapper>